{
  "name": "app",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev": "ts-node-dev --respawn --clear app.ts",
    "build": "rimraf ./dist && tsc",
    "start": "node ./dist/app.js",
    "tsc": "tsc",
    "migrate:generate": "ts-node-dev scripts/create-migrations.ts",
    "migrate:up": "npx sequelize-cli db:migrate",
    "migrate:up:to": "npx sequelize-cli db:migrate --to",
    "migrate:undo:recent": "npx sequelize-cli db:migrate:undo",
    "migrate:undo:to": "npx sequelize-cli db:migrate:undo:all --to",
    "migrate:undo": "npx sequelize-cli db:migrate:undo:all",
    "seed:generate": "npx sequelize-cli seed:generate --name",
    "seed:up": "npx sequelize-cli db:seed:all",
    "seed:up:to": "npx sequelize-cli db:seed --seed",
    "seed:undo:recent": "npx sequelize-cli db:seed:undo",
    "seed:undo:to": "npx sequelize-cli db:seed:undo --seed",
    "seed:undo": "npx sequelize-cli db:seed:undo:all",
    "model:generate": "ts-node-dev scripts/create-models.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "packageManager": "pnpm@10.5.2",
  "dependencies": {
    "bcrypt": "5.1.1",
    "change-case": "5.4.4",
    "cors": "2.8.5",
    "dotenv": "16.5.0",
    "ejs": "^3.1.10",
    "express": "5.1.0",
    "express-ejs-layouts": "^2.5.1",
    "ioredis": "5.6.1",
    "jose": "5.9.6",
    "mysql2": "3.14.1",
    "qs": "6.14.0",
    "rimraf": "6.0.1",
    "sequelize": "6.37.7",
    "zod": "3.24.4"
  },
  "devDependencies": {
    "@types/bcrypt": "5.0.2",
    "@types/cors": "2.8.17",
    "@types/express": "5.0.1",
    "@types/express-ejs-layouts": "^2.5.4",
    "@types/node": "22.15.3",
    "@types/qs": "6.9.18",
    "@types/sequelize": "4.28.20",
    "@types/supertest": "6.0.3",
    "prettier": "3.5.3",
    "sequelize-cli": "6.6.2",
    "ts-node-dev": "2.0.0",
    "typescript": "5.8.3"
  }
}
